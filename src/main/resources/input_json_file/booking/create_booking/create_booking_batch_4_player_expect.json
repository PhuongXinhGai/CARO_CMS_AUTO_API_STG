"$[1].IsCheckedIn": ${assert_[1].IsCheckedIn},
"$[1].bag_status": "${assert_[1].bag_status}",
"$[1].bill_code": "${assert_[1].bill_code}",
"$[1].booking_code": "${assert_[1].booking_code}",
"$[1].customer_type": "${assert_[1].customer_type}",
"$[1].has_book_caddie": ${assert_[1].has_book_caddie},

