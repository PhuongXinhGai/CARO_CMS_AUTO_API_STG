{
  "status_code": ${status_code},

"assertions_explicit": [
{ "path": "$", "op": "size", "value": ${assert_size} }
],

"assertions": {
"$[0].uid": "${assert_[0]_uid}",
"$[0].customer_booking_name": "${assert_[0]_customer_booking_name}",
"$[0].guest_style": "${assert_[0]_guest_style}",
"$[0].IsCheckedIn": ${assert_[0].IsCheckedIn},
"$[0].bag_status": "${assert_[0]_bag_status}",
"$[0].bill_code": "${assert_[0]_bill_code}",
"$[0].booking_code": "${assert_[0]_booking_code}",
"$[0].customer_type": "${assert_[0]_customer_type}",
"$[0].has_book_caddie": ${assert_[0]_has_book_caddie}
},

"extract": {
"booking_uid_0": "$[0].uid",
"booking_code_0": "$[0].booking_code"
}
}
